{
  "manifest_version": 2,

  "name": "Bedflix",
  "description": "Chrome extension that interacts with Netflix.",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "BedFlix"
  },
  "permissions": [
    "activeTab",
    "storage",
    "tabs"
  ]
}